{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "*.js"
  ],
  "exclude": [
    "index.js",
    "internal.js"
  ]
}
